" neobundle で管理しているプラグインを読み込む
source ~/.vim/vimrc/bundle.vimrc

" 基本設定
source ~/.vim/vimrc/basic.vimrc

" インデント設定
source ~/.vim/vimrc/indent.vimrc

" プラグイン設定
source ~/.vim/vimrc/plugin_setting.vimrc

" unite.vim 設定
source ~/.vim/vimrc/unite.vimrc

" 補完設定
source ~/.vim/vimrc/completion.vimrc

" カラー設定
source ~/.vim/vimrc/color.vimrc

" tags 設定
source ~/.vim/vimrc/tags.vimrc

" tab 設定
source ~/.vim/vimrc/tab.vimrc

" split 設定
source ~/.vim/vimrc/split.vimrc
